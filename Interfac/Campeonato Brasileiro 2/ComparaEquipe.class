// comparar as equipes dentro do campeonato

import java.util.Scanner;
import java.util.Arrays;
import java.util.Comparator;

class ComparaEquipe implements Comparator<Equipe> {
    
    public int compare(Equipe t1, Equipe t2) {
        
        // condições para cada equipe pontuar :
        if(t2.totalPoint > t1.totalPoint) return 1;
        else if(t2.totalPoint < t1.totalPoint) return -1;
        else if (t2.totalVictory > t1.totalVictory) return 1;
        else if (t2.totalVictory < t1.totalVictory) return -1;
        else if (t2.balanceGols > t1.balanceGols) return 1;
        else if (t2.balanceGols < t1.balanceGols) return -1;
        else return Integer.compare(t2.golsPro, t1.golsPro);
        
        
    }
    
}